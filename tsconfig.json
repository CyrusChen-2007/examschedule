{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "es2022",
    "moduleResolution": "node",
    "target": "es2022",
    "lib": ["es2022", "dom", "dom.iterable"],
    "types": ["jquery", "jqueryui"]
  }
}